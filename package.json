{
   "name": "latest-nest",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "ts-node src/index.ts",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
      "migration:generate:rento:postgres": "npm run typeorm:cli -- migration:generate -d src/shared/postgres.config-migrations.ts",
      "migration:create:rento:postgres": "npm run typeorm:cli -- migration:create -d src/database/migrations --connection rento_postgres -n",
      "migration:run:rento:postgres": "pnpm typeorm:cli migration:run -d src/shared/postgres.config-migrations.ts",
      "db:drop:rento:postgres": "pnpm typeorm:cli schema:drop -d src/shared/postgres.config-migrations.ts"
   },
   "dependencies": {
      "@nestjs/cache-manager": "^1.0.0",
      "@nestjs/common": "^9.4.0",
      "@nestjs/core": "^9.4.0",
      "@nestjs/passport": "^9.0.3",
      "@nestjs/platform-express": "^9.4.0",
      "@nestjs/typeorm": "^9.0.1",
      "@types/express-session": "^1.17.7",
      "argon2": "^0.30.3",
      "cache-manager": "^5.2.1",
      "cache-manager-redis-store": "^2.0.0",
      "connect-redis": "^6.1.3",
      "dotenv": "^16.0.3",
      "express-session": "^1.17.3",
      "passport": "^0.6.0",
      "passport-local": "^1.0.0",
      "pg": "^8.4.0",
      "redis": "^3.1.2",
      "reflect-metadata": "^0.1.13",
      "rxjs": "^7.8.1",
      "typeorm": "^0.3.16"
   },
   "devDependencies": {
      "@nestjs/cli": "^9.4.2",
      "@nestjs/schematics": "^9.1.0",
      "@nestjs/testing": "^9.4.0",
      "@types/cache-manager": "^4.0.2",
      "@types/cache-manager-redis-store": "^2.0.1",
      "@types/connect-redis": "^0.0.20",
      "@types/express": "^4.17.17",
      "@types/jest": "29.5.0",
      "@types/node": "^16.18.28",
      "@types/passport": "^1.0.12",
      "@types/passport-local": "^1.0.35",
      "@types/redis": "2.8.32",
      "@types/supertest": "^2.0.12",
      "@typescript-eslint/eslint-plugin": "^5.59.5",
      "@typescript-eslint/parser": "^5.59.5",
      "eslint": "^8.40.0",
      "eslint-config-prettier": "^8.8.0",
      "eslint-plugin-prettier": "^4.2.1",
      "jest": "29.5.0",
      "prettier": "^2.8.8",
      "source-map-support": "^0.5.21",
      "supertest": "^6.3.3",
      "ts-jest": "29.0.5",
      "ts-loader": "^9.4.2",
      "ts-node": "10.7.0",
      "tsconfig-paths": "4.2.0",
      "typescript": "4.5.2"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}